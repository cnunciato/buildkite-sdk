{
    "targets": {
        "gen:types": {
            "command": "node sdk/gen-types.js",
            "dependsOn": ["fetch"]
        },
        "test:all": {
            "dependsOn": [
                "sdk-typescript:test",
                "sdk-python:test",
                "sdk-go:test",
                "app-typescript:test",
                "app-python:test",
                "app-go:test"
            ]
        },
        "build:all": {
            "dependsOn": [
                "sdk-typescript:build",
                "sdk-python:build",
                "sdk-go:build"
            ]
        },
        "start:all": {
            "dependsOn": [
                "app-typescript:start",
                "app-python:start",
                "app-go:start"
            ]
        },
        "publish:all": {
            "dependsOn": [
                "sdk-typescript:publish",
                "sdk-python:publish",
                "sdk-go:build"
            ]
        }
    }
}
