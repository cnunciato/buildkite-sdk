{
    "name": "lib-go",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/sdk/go/pkg/buildkite",
    "tags": [],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    "rm -rf dist/libs/sdk/go/*",
                    "mkdir -p dist/libs/sdk/go",
                    "cp -R libs/sdk/go/* dist/libs/sdk/go/"
                ]
            },
            "cache": false
        },
        "publish": {
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    "go list -m github.com/cnunciato/buildkite-sdk@v0.0.10"
                ],
                "env": {
                    "GOPROXY": "proxy.golang.org"
                }
            },
            "cache": false
        },
        "test": {
            "executor": "@nx-go/nx-go:test"
        },
        "lint": {
            "executor": "@nx-go/nx-go:lint"
        }
    }
}
