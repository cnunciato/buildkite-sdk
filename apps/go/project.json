{
    "name": "app-go",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/go",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx-go/nx-go:build",
            "options": {
                "main": "{projectRoot}/main.go"
            }
        },
        "run": {
            "executor": "@nx-go/nx-go:serve",
            "options": {
                "main": "{projectRoot}/main.go"
            }
        },
        "test": {
            "executor": "@nx-go/nx-go:test"
        }
    }
}
